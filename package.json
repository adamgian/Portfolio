{
    "name": "portfolio",
    "version": "0.0.29",
    "author": {
        "name": "Adam Gian",
        "email": "contact@adamgian.com",
        "url": "adamgian.com"
    },
    "license": "ISC",
    "private": true,
    "scripts": {
        "clean": "rimraf dist/",
        "start": "npm run dev",
        "build": "npm run clean && npm run build:webpack && npm run build:jekyll && npm run build:html",
        "dev": "npm run clean && concurrently 'npm run serve:webpack' 'npm run serve:jekyll'",
        "build:html": "html-minifier --input-dir 'dist/' --output-dir 'dist/' --file-ext 'html' --remove-comments --collapse-whitespace --minify-js --minify-css --sort-attributes --sort-class-name",
        "build:jekyll": "cross-env JEKYLL_ENV=production jekyll build --config config/_config.yaml",
        "build:webpack": "cross-env NODE_ENV=production webpack --config config/webpack-prod.config.js",
        "serve:jekyll": "cross-env JEKYLL_ENV=development jekyll serve --config config/_config.yaml,config/_config-dev.yaml --watch",
        "serve:webpack": "cross-env NODE_ENV=development webpack --config config/webpack-dev.config.js --watch",
        "test": "eslint -c config/.eslintrc src/assets/javascript --ignore-path config/.eslintignore"
    },
    "dependencies": {
        "@babel/core": "^7.12.9",
        "@babel/preset-env": "^7.12.7",
        "autoprefixer": "^9.8.6",
        "babel-loader": "^8.2.1",
        "browserslist": "^4.14.7",
        "caniuse-lite": "^1.0.30001161",
        "concurrently": "^5.3.0",
        "core-js": "^3.7.0",
        "cross-env": "^5.2.1",
        "css-loader": "^2.1.1",
        "cssnano": "^4.1.10",
        "eslint": "^5.16.0",
        "html-minifier": "^4.0.0",
        "mini-css-extract-plugin": "^0.5.0",
        "node-sass": "^4.14.1",
        "path": "^0.12.7",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "rimraf": "^2.7.1",
        "sass-loader": "^7.3.1",
        "style-loader": "^0.23.1",
        "webfontloader": "^1.6.28",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12"
    },
    "engines": {
        "node": ">=14.4.0",
        "npm": ">=6.14.5"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
